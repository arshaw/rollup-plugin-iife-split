{
  "name": "rollup-plugin-iife-split",
  "version": "1.0.0",
  "type": "module",
  "description": "Rollup plugin for intelligent IIFE code-splitting",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup src/index.ts --format esm --dts",
    "test": "vitest",
    "test:run": "vitest run",
    "prepublishOnly": "pnpm run build && pnpm run test:run"
  },
  "peerDependencies": {
    "rollup": "^3.0.0 || ^4.0.0"
  },
  "dependencies": {
    "magic-string": "^0.30.0",
    "acorn": "^8.11.0",
    "estree-walker": "^3.0.0"
  },
  "devDependencies": {
    "rollup": "^4.0.0",
    "typescript": "^5.0.0",
    "tsup": "^8.0.0",
    "vitest": "^1.0.0",
    "@types/node": "^20.0.0"
  },
  "keywords": ["rollup", "plugin", "iife", "code-splitting", "bundle"],
  "license": "MIT"
}
